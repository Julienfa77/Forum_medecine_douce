{% extends 'base.html.twig' %}

{% block title %}{{ 'Categorie' | trans }}{% endblock %}

{% block body %}
    <h1>{{ 'Categorie' | trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categorie.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ categorie.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('categorie_index') }}">{{ 'back to list' | trans }}</a>

    <a href="{{ path('categorie_edit', {'id': categorie.id}) }}">{{ 'edit' | trans }}</a>

    {{ include('categorie/_delete_form.html.twig') }}
{% endblock %}
